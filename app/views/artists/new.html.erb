<%= render 'shared/navbar' %>

<h1>Create new artist</h1>

<%= simple_form_for @artist, url: create_artist_path do |f| %>
  <%= f.input :name %>
  <%= f.input :quote %>
  <%= f.input :quoted_by %>
  <%= f.input :spotify %>
  <%= f.input :instagram %>
  <%= f.input :manage %>
  <%= f.input :recording %>
  <%= f.input :photo, as: :file %>
  <%= f.submit value: "Create" %>
<% end %>
